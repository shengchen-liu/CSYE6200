package edu.neu.csye6200;


public class PersonFactory {

	private PersonFactory() {
		
	}
	
	public static PersonAPI getObject(int criteria){
		PersonAPI obj = null;
		if (criteria == 0) {
			obj = new Student();
		}
		else {
			obj = new Employee();
		}
		return obj;
		
	}
	//getPerson () //employee, student
}

	
